{
  "expo": {
    "name": "WaList",
    "slug": "WaList",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon2.png",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#44475a"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon2.png",
        "backgroundColor": "#44475a"
      },
      "package": "com.walefy.walist"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
